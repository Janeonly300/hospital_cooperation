<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hl.hos.mapper.TemplateMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.hl.hos.pojo.Template">
        <id column="id" property="id" />
        <result column="doctor_id" property="doctor_id" />
        <result column="file_name" property="file_name" />
        <result column="file_addr" property="file_addr" />
        <result column="type" property="type" />
        <result column="creat_time" property="creat_time" />
        <result column="stauts" property="stauts" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, doctor_id, file_name, file_addr, type, creat_time, stauts
    </sql>

</mapper>
